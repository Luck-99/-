    <view class="fuhexing">{{tizhi2}}</view>
</view>
<view class="categoryv">
    <image class="categoryicon" src="/resources/shexiang.png"></image>
</view>
<view>
    <view>
        <image class="typeimg2" mode="scaleToFill" src="{{typeimg2}}"></image>
    </view>
    <view>
        <view class="flex-column" id="panel">
            <view class="flex-cell flex-row" style="color: white;background-color: #C79A5B;">
                <text class="flex-cell flex-row">{{cha1}}</text>
                <text class="flex-cell flex-row">{{cha2}}</text>
                <text class="flex-cell flex-row">{{cha3}}</text>
                <text class="flex-cell flex-row">{{cha4}}</text>
                <text class="flex-cell flex-row">{{cha5}}</text>
            </view>
            <view class="flex-row flex-cell" style="color: white;background-color: #D8BB93;">
                <text class="flex-cell flex-row">{{char1}}</text>
                <text class="flex-cell flex-row">{{char2}}</text>
                <text class="flex-cell flex-row">{{char3}}</text>
                <text class="flex-cell flex-row">{{char4}}</text>
                <text class="flex-cell flex-row">{{char5}}</text>
            </view>
        </view>
    </view>
    <view>
        <view class="flex-column" id="panel">
            <view class="flex-cell flex-row" style="color: white;background-color: #C79A5B;">
                <text class="flex-cell flex-row">{{cha6}}</text>
                <text class="flex-cell flex-row">{{cha7}}</text>
                <text class="flex-cell flex-row">{{cha8}}</text>
                <text class="flex-cell flex-row">{{cha9}}</text>
                <text class="flex-cell flex-row">{{cha10}}</text>
            </view>
            <view class="flex-row flex-cell" style=" color: white;background-color: #D8BB93;">
                <text class="flex-cell flex-row">{{char6}}</text>
                <text class="flex-cell flex-row">{{char7}}</text>
                <text class="flex-cell flex-row">{{char8}}</text>
                <text class="flex-cell flex-row">{{char9}}</text>
                <text class="flex-cell flex-row">{{char10}}</text>
            </view>
        </view>
    </view>
</view>
<view class="jibenxx" wx:for="{{resultobj}}" wx:key="item">
    <view class="typev">
        <image class="typeimg" mode="widthFix" src="{{item.tizhitou_img}}"></image>
    </view>
    <view class="categoryv">
        <image class="categoryicon" src="/resources/tiaolifangfa.png"></image>
    </view>
    <view class="biaoti2">
        <text class="text2">食疗</text>
    </view>
    <view class="describet">{{item.shiliao}}</view>
    <view class="biaoti2">
        <text class="text2">情志调养</text>
    </view>
    <view class="describet">{{item.jingshentiaoyang}}</view>
    <view class="biaoti2">
        <text class="text2">娱乐调摄</text>
    </view>
    <view class="describet">{{item.yuletiaoshe}}</view>
    <view class="biaoti2">
        <text class="text2">四季养生</text>
    </view>
    <view class="describet">{{item.sijiyangsheng}}</view>
    <view class="biaoti2">
        <text class="text2">体育锻炼</text>
    </view>
    <view class="describet">{{item.tiyuduanlian}}</view>
    <view class="biaoti2">
        <text class="text2">起居调摄</text>
    </view>
    <view class="describet">{{item.qijutiaoshe}}</view>
    <view class="biaoti2">
        <text class="text2">音乐调理</text>
    </view>
    <view class="describet">{{item.yinyuetiaoli}}</view>
    <view class="biaoti2">
        <text class="text2">经络保健</text>
    </view>
    <view class="describet">{{item.jingluobaojian}}</view>
    <view class="recommendgoodsv" style="display:none">
        <image class="kuangimg" src="/resources/kuang.png"></image>
        <view bindtap="goshop" class="goodsv" data-id="{{item.tea_path}}">
            <image class="goodsimg" mode="aspectFill" src="{{item.tea_img}}"></image>
            <view class="goodsinfo">
                <view class="tipv">根据您的体质为您推荐</view>
                <view class="goodsname">{{item.recommend_tea}}</view>
                <view class="pricev">
                    <view class="pricetip">首次购买</view>
                    <view class="buyprice">¥ <text class="numt">{{item.tea_price}}</text>
                    </view>
                    <view class="originprice">{{item.tea_price}}</view>
                </view>
                <view class="buyv">
                    <image class="handicon" src="/resources/left.gif"></image>
                    <view class="buytxt">去购买</view>
                    <image class="handicon" src="/resources/right.gif"></image>
                </view>
            </view>
        </view>
    </view>
</view>
<image bindtap="shareopt" class="shareimg" mode="widthFix" src="/resources/call.png"></image>
<image bindtap="tiaofeed" class="feed" src="/resources/feedimg.gif"></image>
<view bindtap="hidewindow" catchtouchmove="nomoveopt" class="service_boxs mask" wx:if="{{showwindow}}">
    <view class="show_qiaodao_jifen">
        <view class="contentxt">
            <canvas canvasId="myCanvas" class="canvascss"></canvas>
        </view>
        <view class="btnv">
            <image class="downpng" src="/resources/xiazai.png"></image>
            <view bindtap="saveopt" class="btnokv">点击保存图片</view>
        </view>
    </view>
</view>
