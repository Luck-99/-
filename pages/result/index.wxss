page {
    background: #FCF7F8;
  }
  
  .resultback{
    width: 100%;
    height: 480rpx;
    position: relative;
    z-index: -1;
  }
  
  .history {
    width: 100vw;
    height: 200rpx;
    display: flex;
    position: relative;
    top: 0;
    left: 0;
    z-index: 2;
  }
  
  .historypic {
    overflow: hidden;
    display: block;
    width: 48rpx;
    height: 48rpx;
    margin: -60% 0rpx 0rpx 20rpx;
  
  }
  
  .historyname {
    display: flex;
    font-size: 30rpx;
    margin-left: 15rpx;
    color: #7dc5eb;
    margin-top: -60%;
  }
  
  
  .duodan {
    opacity: 1;
    text-align: center;
    margin:-75% 0 5% 0%;
  }
  
  .fuhexing {
    font-size: 46rpx;
    color: #000000a1;
  }
  
  .jibenxx {
    margin-top: 320rpx;
  }
  
  
  .describet {
    font-size: 28rpx;
    color: #333;
    margin: 25rpx 40rpx;
  }
  
  .biaoti {
    border-left: 6rpx solid #D9A272;
    color: #4F8CCE;
    font-size: 30rpx;
    margin-left: 34rpx;
  }
  
  .text {
    margin-left: 10rpx;
  }
  
  #panel {
    height: 8vh;
    background: #00000000;
    width: 62vw;
    float: left;
    margin-left: 5vw;
    margin-top: 2vh;
  }
  
  #panel text {
    line-height: 4vh;
    border-right: 1rpx solid #88622Ddd;
    border-bottom: 1rpx solid #88622Ddd;
    font-size: 22rpx;
  }
  
  .flex-row {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border-radius: 6%;
  }
  
  .flex-column {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: stretch;
  }
  
  .flex-cell {
    flex: 1;
  }
  
  .typeimg {
    width: 23vw;
    margin-top: 2vh;
    height: 18vh;
    background-color: black;
    float: left;
    margin-left: 5vw;
  }
  
  
  .flex-column-bottom {
    width: 100%;
    flex-direction: column;
  }
  
  #panel-bottom {
    width: 98%;
  }
  
  .flex-row-bottom {
    display: flex;
    flex-direction: row;
  }
  
  .buttompic{
    height: 25vw;
    margin:2% 0% 0% 2%;
  }
  
  .buttompic2{
    height: 22vw;
    margin:2% 0% 0% 2%;
  }